# Example environment file for server
# Copy this to `.env` and fill in real values

# MongoDB connection string (local or Atlas)
# For local MongoDB (default):
# MONGODB_URI=mongodb://localhost:27017/resume_builder_db

# Or if you use a different variable name elsewhere you can set DATABASE_URL instead
# DATABASE_URL=mongodb://localhost:27017/resume_builder_db

# Server port
PORT=5000

# JWT secret (example)
JWT_SECRET=your_jwt_secret_here
